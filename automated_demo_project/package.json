{
  "name": "dinara-project",
  "version": "1.0.0",
  "description": "Playwright E2E testing project using Page Object Model pattern",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:david": "playwright test tests/specs/davidJeremiahWorkflow.spec.ts",
    "test:all-browsers": "BROWSER_ONLY=all playwright test",
    "test:david-all": "BROWSER_ONLY=all playwright test tests/specs/davidJeremiahWorkflow.spec.ts",
    "install:browsers": "playwright install",
    "lint": "eslint src/ tests/ --ext .ts",
    "lint:fix": "eslint src/ tests/ --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "allure:generate": "npx allure generate allure-results --clean",
    "allure:open": "npx allure open allure-report",
    "allure:serve": "npx allure serve allure-results"
  },
  "keywords": [
    "playwright",
    "e2e",
    "testing",
    "page-object-model",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "allure-commandline": "^2.34.1",
    "allure-playwright": "^3.3.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-playwright": "^0.18.0",
    "prettier": "^3.0.0",
    "typescript": "^5.9.2"
  }
}
